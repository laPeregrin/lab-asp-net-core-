@model ObjectContainer.Objects.Contact

@{
    ViewData["Title"] = "ADMIN";
}

<h4>Contact</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="LoginAdmin">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
